mod language_examples {
    fn allocating_memory() *u8 {
        let p *u8
        p = alloc(u32(1))
        *p = 0
        return p
    }

    fn while_loop(s *u8) u32 {
        let i u32
        while (*(s + i)) != u8(0) {
            i = i + u32(1)
        }
        return i
    }

    fn pointer_conversions() {
        let p **u8
        p = (**u8)(u32(0x3000_0000))
        let a u32
        a = u32((*(p)))
    }
}

mod main {
    fn main(args **u8) u8 {
    }
}